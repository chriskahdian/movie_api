ct","generated":{"line":141,"column":2},"source":"components/main-view/main-view.jsx","original":{"line":12,"column":30}},{"name":"Component","generated":{"line":141,"column":17},"source":"components/main-view/main-view.jsx","original":{"line":12,"column":36}}],"sources":{"components/main-view/main-view.jsx":"import React from \"react\";\r\nimport axios from \"axios\";\r\nimport Container from \"react-bootstrap/Container\";\r\nimport Col from \"react-bootstrap/Col\";\r\nimport Row from \"react-bootstrap/Row\";\r\n\r\nimport { LoginView } from \"../login-view/login-view\";\r\nimport { RegistrationView } from \"../registration-view/registration-view\";\r\nimport { MovieCard } from \"../movie-card/movie-card\";\r\nimport { MovieView } from \"../movie-view/movie-view\";\r\n\r\nexport class MainView extends React.Component {\r\n  constructor() {\r\n    super();\r\n    this.state = {\r\n      movies: null,\r\n      selectedMovie: null,\r\n      user: null,\r\n    };\r\n  }\r\n\r\n  componentDidMount() {\r\n    axios\r\n      .get(\"https://myflix001.herokuapp.com/movies\")\r\n      .then((response) => {\r\n        //assign the result to the state\r\n        this.setState({\r\n          movies: response.data,\r\n        });\r\n      })\r\n      .catch(function (error) {\r\n        console.log(error);\r\n      });\r\n  }\r\n\r\n  onMovieClick(movie) {\r\n    this.setState({\r\n      selectedMovie: movie,\r\n    });\r\n  }\r\n\r\n  clearState() {\r\n    this.setState({\r\n      selectedMovie: null,\r\n    });\r\n  }\r\n\r\n  onLoggedIn(user) {\r\n    this.setState({\r\n      user,\r\n    });\r\n  }\r\n\r\n  render() {\r\n    const { movies, selectedMovie, user } = this.state;\r\n    if (!user)\r\n      return <LoginView onLoggedIn={(user) => this.onLoggedIn(user)} />;\r\n    if (!movies) return <div className=\"main-view\" />;\r\n    return (\r\n      <div className=\"main-view\">\r\n        <Container>\r\n          <Row>\r\n            {/* <Col> */}\r\n              {selectedMovie ? (\r\n                <MovieView\r\n                  movie={selectedMovie}\r\n                  clearState={() => this.clearState()}\r\n                />\r\n              ) : (\r\n                movies.map((movie) => (\r\n                  <MovieCard\r\n                    key={movie._id}\r\n                    movie={movie}\r\n                    onClick={(movie) => this.onMovieClick(movie)}\r\n                  />\r\n                ))\r\n              )}\r\n            {/* </Col> */}\r\n          </Row>\r\n        </Container>\r\n      </div>\r\n    );\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"79fc47063e29d493816ad13e27b34723","cacheData":{"env":{}}}